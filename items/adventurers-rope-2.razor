# @setvar yellow_dg_gate_hue 2822
# @setvar red_dg_gate_hue 2820

if not findtype 'rope' 'backpack'
    overhead 'no rope!'
    stop
endif

if not targetexists 'neutral'
    overhead 'using rope'
    usetype 'rope'
else
    if findtype 'blue moongate' ground 2822 any 20 as field
        overhead '*targetting yellow gate*' 56
        target field
    elseif findtype 'blue moongate' ground 2820 any 20 as field
        overhead '*targetting red gate*' 56
        target field
    endif
endif
