if findtype 'ornate elven chest' ground any any 2 as 'recycler'
    sysmsg "> Dumping itens in recycler"
    useobject 'recycler'
    waitforgump 4045111101
    gumpresponse 3
    waitfortarget
    target 'self'
    waitforgump 4045111101
    gumpclose
else
    overhead 'no recyclers nearby'
endif
